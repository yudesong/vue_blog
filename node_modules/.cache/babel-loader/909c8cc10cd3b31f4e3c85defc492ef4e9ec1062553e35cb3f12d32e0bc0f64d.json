   <div class=\"py-2\">\n            <input type=\"text\" placeholder=\"搜索文章...\" class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-indigo-500\">\n          </div>\n          <button @click=\"toggleAdminView; mobileMenuOpen = false\" class=\"bg-indigo-600 hover:bg-indigo-700 text-white px-4 py-2 rounded-md transition-colors flex items-center justify-center\">\n            <i class=\"fa fa-cog mr-2\"></i>\n            <span>{{ isAdminView ? '返回前台' : '管理后台' }}</span>\n          </button>\n        </div>\n      </div>\n    </nav>\n  </header>\n</template>\n\n<script>\nexport default {\n  name: 'BlogHeader',\n  props: {\n    currentView: {\n      type: String,\n      required: true\n    },\n    isAdminView: {\n      type: Boolean,\n      required: true\n    },\n    isScrolled: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      showMobileMenu: false\n    }\n  },\n  methods: {\n    toggleMobileMenu() {\n      this.showMobileMenu = !this.showMobileMenu\n    },\n    switchView(view) {\n      this.$emit('switch-view', view)\n    },\n    toggleAdminView() {\n      this.$emit('toggle-admin-view')\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 导航栏样式已集成在组件中 */\n</style>"],"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,WAAW,EAAE;MACXC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,WAAW,EAAE;MACXH,IAAI,EAAEI,OAAO;MACbF,QAAQ,EAAE;IACZ,CAAC;IACDG,UAAU,EAAE;MACVL,IAAI,EAAEI,OAAO;MACbF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACF,cAAa,GAAI,CAAC,IAAI,CAACA,cAAa;IAC3C,CAAC;IACDG,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACC,KAAK,CAAC,aAAa,EAAED,IAAI;IAChC,CAAC;IACDE,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACD,KAAK,CAAC,mBAAmB;IAChC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}